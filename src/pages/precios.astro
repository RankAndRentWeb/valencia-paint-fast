---
import Layout from "../layouts/Layout.astro";
import Precios from "../components/_Precios.tsx";

const title = "Precios de pintura en Valencia";
const description = "Consulta nuestras tarifas de pintura. Precios competitivos y transparentes.";
const canonical = new URL(Astro.url.pathname, Astro.site).toString();
---

<Layout {title} {description} {canonical}>
  <Precios client:load />
</Layout>