---
import Layout from "../layouts/Layout.astro";
import Servicios from "../components/_Servicios.tsx";

const title = "Servicios de pintura en Valencia";
const description = "Interior, exterior, gotelé, impermeabilización…";
const canonical = new URL(Astro.url.pathname, Astro.site).toString();
---

<Layout {title} {description} {canonical}>
  <Servicios client:load />
</Layout>