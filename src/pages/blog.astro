---
import Layout from "../layouts/Layout.astro";
import Blog from "../components/_Blog.tsx";

const title = "Blog de Pintura Valencia | Consejos y Tendencias";
const description = "Blog con consejos de pintura, tendencias de color y guías para tu hogar.";
const canonical = new URL(Astro.url.pathname, Astro.site).toString();
---

<Layout {title} {description} {canonical}>
  <Blog client:load />
</Layout>
