---
import Layout from '../layouts/Layout.astro';
import Blog from '../components/_Blog.tsx';

const title = "Blog de Pintura Valencia | Consejos y Tendencias";
const description = "Blog con consejos de pintura, tendencias de color y guías para tu hogar. Expertise de nuestros pintores profesionales.";
const canonicalUrl = "https://pintores-valencia.com/blog";
---

<Layout {title} {description} {canonicalUrl}>
  <Blog client:load />
</Layout>
