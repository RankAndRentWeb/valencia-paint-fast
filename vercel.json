{
  "cleanUrls": true,
  "headers": [
    { "source": "/assets/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
    { "source": "/(.*\\.(?:jpg|jpeg|png|webp|avif|gif|svg|ico|css|js|woff2?))", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] }
  ],
  "redirects": [
    { "source": "/quitar-gotele-alisar-paredes", "destination": "/servicios/quitar-gotele/", "permanent": true },
    { "source": "/quitar-gotele-valencia", "destination": "/servicios/quitar-gotele/", "permanent": true },
    { "source": "/pintura-interior-valencia", "destination": "/servicios/pintura-interior/", "permanent": true },
    { "source": "/pintura-interior-pisos-casas", "destination": "/servicios/pintura-interior/", "permanent": true },
    { "source": "/pintura-exterior-fachadas", "destination": "/servicios/pintura-exterior/", "permanent": true },
    { "source": "/impermeabilizacion-terrazas", "destination": "/servicios/impermeabilizacion/", "permanent": true },
    { "source": "/impermeabilizacion-terrazas-y-fachadas", "destination": "/servicios/impermeabilizacion/", "permanent": true },
    { "source": "/pintura-locales-comerciales", "destination": "/servicios/pintura-locales-comerciales/", "permanent": true },
    { "source": "/pintura-comunidades", "destination": "/servicios/pintura-comunidades/", "permanent": true },
    { "source": "/blog", "destination": "/blogs/", "permanent": true },
    { "source": "/blog/:category", "destination": "/blogs/:category/", "permanent": true },
    { "source": "/blog/:category/:slug", "destination": "/blogs/:category/:slug/", "permanent": true },
    { "source": "/sitemap.xml", "destination": "/sitemap-index.xml", "permanent": true }
  ]
}